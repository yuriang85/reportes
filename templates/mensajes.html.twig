{% for message in app.flashes('success') %}
<div class="alert alert-success">
    {{ message }}
</div>
{% endfor %}
{% for message in app.flashes('error') %}
<div class="alert alert-error">
    {{ message }}
</div>
{% endfor %}
{% endfor %}
{% for message in app.flashes('warning') %}
<div class="alert alert-warning">
    {{ message }}
</div>
{% endfor %}
